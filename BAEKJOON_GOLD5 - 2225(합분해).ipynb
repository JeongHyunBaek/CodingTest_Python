   "source": [
    "n, k = map(int, input().split())\n",
    "dp = [[0] * (k + 1) for _ in range(n + 1)]\n",
    "dp[0][0] = 1\n",
    "\n",
    "for i in range(n + 1):\n",
    "    for j in range(1, k + 1):\n",
    "        dp[i][j] = dp[i - 1][j] + dp[i][j - 1]\n",
    "        \n",
    "print(dp[n][k] % 1000000000)"
   ]
  }
